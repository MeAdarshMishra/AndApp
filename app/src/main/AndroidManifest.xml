<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.andapp">

    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndApp">
        <activity
            android:name=".Camera"
            android:exported="true" />
        <activity
            android:name=".Calculator"
            android:exported="true" />
        <activity
            android:name=".Second"
            android:exported="true" />
        <activity android:name=".AllApp" />
        <activity android:name=".OnlineLogin" />
        <activity android:name=".DatabasePage" />
        <activity android:name=".OfflineSignup" />
        <activity android:name=".OfflineLogin" />
        <activity android:name=".LoginPage" />
        <activity android:name=".Bluetooth" />
        <activity android:name=".AccelerometerFlash" />
        <activity android:name=".AccelerometerWifi" />
        <activity android:name=".AccelerometerBluetooth" />
        <activity android:name=".FlashLight" />
        <activity android:name=".TtSpeech" />
        <activity android:name=".AccelerometerSensor" />
        <activity android:name=".VibrationSensor" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>